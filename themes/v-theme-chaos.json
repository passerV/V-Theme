{
    "$schema": "vscode://schemas/color-theme",
    "type": "dark",
    "colors": {
        "activityBar.background": "#2f2b26",
        "activityBar.foreground": "#918f8f",
        "activityBarBadge.background": "#d76538",
        "debugIcon.breakpointForeground": "#d76538",
        "editor.background": "#2f2b26",
        "editor.foreground": "#ebd1b7",
        "editor.lineHighlightBackground": "#45403b",
        "editor.selectionBackground": "#db784d4d",
        "editorBracketHighlight.foreground1": "#d76538",
        "editorBracketHighlight.foreground2": "#f8bb39",
        "editorBracketHighlight.foreground3": "#00a8c6",
        "editorBracketHighlight.foreground4": "#95cc5e",
        "editorBracketHighlight.foreground5": "#e61f44",
        "editorBracketHighlight.foreground6": "#d76538",
        "editorCursor.foreground": "#f8f8f0",
        "editorError.foreground": "#d76538",
        "editorInfo.foreground": "#00a8c6",
        "editorWarning.foreground": "#f8bb39",
        "editorWhitespace.foreground": "#3b3a32",
        "sash.hoverBorder": "#db784d85",
        "sideBar.background": "#2f2b26",
        "sideBarSectionHeader.background": "#26221e",
        "statusBar.background": "#36312c",
        "statusBar.debuggingForeground": "#ffffff",
        "statusBar.foreground": "#918f8f",
        "statusBar.noFolderBackground": "#2f2b26",
        "tab.activeBackground": "#64594e",
        "tab.inactiveBackground": "#2f2b26",
    },
    "tokenColors": [
        {
            "scope": "comment",
            "settings": {
                "foreground": "#7A7267"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#F8BB39"
            }
        },
        {
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#F8BB39"
            }
        },
        {
            "scope": "constant.language",
            "settings": {
                "foreground": "#DB784D"
            }
        },
        {
            "scope": [
                "constant.character",
                "constant.other"
            ],
            "settings": {
                "foreground": "#DB784D"
            }
        },
        {
            "scope": "variable",
            "settings": {
                "fontStyle": ""
            }
        },
        {
            "scope": "keyword",
            "settings": {
                "foreground": "#DB784D"
            }
        },
        {
            "scope": "storage",
            "settings": {
                "foreground": "#DB784D",
                "fontStyle": ""
            }
        },
        {
            "scope": [
                "storage.type",
                "keyword.other.fn.rust",
                "keyword.other.rust"
            ],
            "settings": {
                "foreground": "#95CC5E",
                "fontStyle": ""
            }
        },
        {
            "scope": "entity.name.class",
            "settings": {
                "foreground": "#DB784D",
                "fontStyle": "underline"
            }
        },
        {
            "scope": "entity.other.inherited-class",
            "settings": {
                "foreground": "#DB784D",
                "fontStyle": "italic underline"
            }
        },
        {
            "scope": "entity.name.function",
            "settings": {
                "foreground": "#60A365",
                "fontStyle": ""
            }
        },
        {
            "scope": "variable.parameter",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#95CC5E",
                "fontStyle": ""
            }
        },
        {
            "scope": "entity.other.attribute-name",
            "settings": {
                "foreground": "#DB784D",
                "fontStyle": ""
            }
        },
        {
            "scope": "support.function",
            "settings": {
                "foreground": "#95CC5E",
                "fontStyle": ""
            }
        },
        {
            "scope": "support.constant",
            "settings": {
                "foreground": "#DB784D",
                "fontStyle": ""
            }
        },
        {
            "scope": [
                "support.type",
                "support.class"
            ],
            "settings": {
                "foreground": "#DB784D",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "support.other.variable",
            "settings": {
                "fontStyle": ""
            }
        },
        {
            "scope": "invalid",
            "settings": {
                "foreground": "#F8F8F0",
                "fontStyle": ""
            }
        },
        {
            "scope": "invalid.deprecated",
            "settings": {
                "foreground": "#F8F8F0"
            }
        },
        {
            "scope": "meta.structure.dictionary.json string.quoted.double.json",
            "settings": {
                "foreground": "#F8BB39"
            }
        },
        {
            "scope": [
                "meta.diff",
                "meta.diff.header"
            ],
            "settings": {
                "foreground": "#75715E"
            }
        },
        {
            "scope": "markup.deleted",
            "settings": {
                "foreground": "#00A8C6"
            }
        },
        {
            "scope": "markup.inserted",
            "settings": {
                "foreground": "#A6E22E"
            }
        },
        {
            "scope": "markup.changed",
            "settings": {
                "foreground": "#E6DB74"
            }
        },
        {
            "scope": "constant.numeric.line-number.find-in-files - match",
            "settings": {
                "foreground": "#8FBE00A0"
            }
        },
        {
            "scope": "entity.name.filename.find-in-files",
            "settings": {
                "foreground": "#E6DB74"
            }
        },
        {
            "scope": "keyword.other",
            "settings": {
                "foreground": "#9A9082"
            }
        },
        {
            "scope": [
                "meta.property-value",
                "support.constant.property-value",
                "constant.other.color"
            ],
            "settings": {
                "foreground": "#F8BB39"
            }
        },
        {
            "scope": "meta.structure.dictionary.json string.quoted.double.json",
            "settings": {
                "foreground": "#DB784D"
            }
        },
        {
            "scope": "meta.structure.dictionary.value.json string.quoted.double.json",
            "settings": {
                "foreground": "#F8BB39"
            }
        },
        {
            "scope": "meta.property-name support.type.property-name",
            "settings": {
                "fontStyle": ""
            }
        },
        {
            "scope": "meta.property-value punctuation.separator.key-value",
            "settings": {
                "foreground": "#EBD1B7"
            }
        },
        {
            "scope": [
                "keyword.other.use",
                "keyword.other.function.use",
                "keyword.other.namespace",
                "keyword.other.new",
                "keyword.other.special-method",
                "keyword.other.unit",
                "keyword.other.use-as"
            ],
            "settings": {
                "foreground": "#95CC5E"
            }
        },
        {
            "scope": [
                "meta.use support.class.builtin",
                "meta.other.inherited-class support.class.builtin"
            ],
            "settings": {
                "foreground": "#95CC5E",
                "fontStyle": ""
            }
        },
        {
            "scope": "variable.other",
            "settings": {
                "fontStyle": ""
            }
        },
        {
            "scope": "variable.parameter.function.coffee",
            "settings": {
                "foreground": "#F8BB39",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "entity.name.section.markdown",
            "settings": {
                "foreground": "#DB784D"
            }
        },
        {
            "scope": "punctuation.definition.heading.markdown",
            "settings": {
                "foreground": "#DB784D"
            }
        },
        {
            "scope": "markup.raw.inline.markdown",
            "settings": {
                "foreground": "#F8BB39"
            }
        },
        {
            "scope": [
                "punctuation.definition.bold.markdown",
                "punctuation.definition.italic.markdown"
            ],
            "settings": {
                "foreground": "#DB784D"
            }
        },
        {
            "scope": [
                "punctuation.definition.string.begin.markdown",
                "punctuation.definition.string.end.markdown"
            ],
            "settings": {
                "foreground": "#DB784D"
            }
        },
        {
            "scope": "punctuation.definition.metadata.markdown",
            "settings": {
                "foreground": "#DB784D"
            }
        },
        {
            "scope": [
                "markup.underline.link.markdown",
                "markup.underline.link.image.markdown",
                "meta.image.inline.markdown"
            ],
            "settings": {
                "foreground": "#95CC5E",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "markup.bold.markdown",
                "markup.italic.markdown"
            ],
            "settings": {
                "foreground": "#95CC5E"
            }
        },
        {
            "scope": "markup.italic.markdown",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "markup.bold.markdown",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": "markup.raw.block.markdown",
            "settings": {
                "foreground": "#00A8C6"
            }
        },
        {
            "scope": "markup.deleted.git_gutter",
            "settings": {
                "foreground": "#E61F44"
            }
        },
        {
            "scope": "markup.inserted.git_gutter",
            "settings": {
                "foreground": "#A7DA1E"
            }
        },
        {
            "scope": "markup.changed.git_gutter",
            "settings": {
                "foreground": "#F7B83D"
            }
        },
        {
            "scope": "token.info-token",
            "settings": {
                "foreground": "#6796E6"
            }
        },
        {
            "scope": "token.warn-token",
            "settings": {
                "foreground": "#CD9731"
            }
        },
        {
            "scope": "token.error-token",
            "settings": {
                "foreground": "#F44747"
            }
        },
        {
            "scope": "token.debug-token",
            "settings": {
                "foreground": "#B267E6"
            }
        }
    ]
}